rem This code convert character to ascii decimal value
rem example; input = A, output = 65
set input=A
set target=%input:~0,1%
 
    for /L %%i in (32, 1, 126) do (
        cmd /c exit /b %%i
        set Chr=^!=ExitCodeAscii!
        if [^!Chr!] EQU [^!target!] set code=%%i & goto END
    )
 
:END
    echo !code!
    
PAUSE
